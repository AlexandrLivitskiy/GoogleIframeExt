{
  "name": "GoogleIframeExt",
  "description": "Sample Chrome Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "webRequest"
  ],
  "content_scripts": [ {
      "matches": ["*://*.olx.ua/*"],
      "js": ["content.js"]
    } ],
  "browser_action": {
    "default_title": "GoogleIframeExt"
  },
  "manifest_version": 2
}
